    "def jumpingOnClouds(c, k):\n",
    "    e=100\n",
    "    n=len(c)\n",
    "    i=0\n",
    "    a=12\n",
    "    while(a!=0):\n",
    "        a=(i+k)%n\n",
    "        i=a\n",
    "        if(c[i]==1):\n",
    "            e=e-2\n",
    "        e=e-1\n",
    "    return e"
   ]
